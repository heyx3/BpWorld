box = Box(
    layer = 0x2,
    min = { 0.065, 0.065, 0.065 },
    max = { 0.435 }.xxx
)
sph = Sphere(
    layer = 0x3,
    center = { {0.25}.xx, 0.75 },
    radius = 0.3
)
terrain = BinaryField(
    layer = 0x1,
    field = 0.2 + (0.5 * perlin(pos * 5.0))
)

# Remove any terrain near the box or sphere.
box_inflated = copy(box,
    min /= 1.3,
    max *= 1.3
    # Alternate syntax: `size *= 1.3`
)
sphere_inflated = copy(sphere,
    radius *= 1.3
)
terrain = Difference(
    terrain,
    [ box_inflated, sphere_inflated ]
)

return Union(box, sph, terrain)